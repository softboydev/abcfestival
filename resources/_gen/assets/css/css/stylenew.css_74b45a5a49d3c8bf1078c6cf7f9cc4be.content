html,body,div,span,applet,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,a,abbr,acronym,address,big,cite,code,del,dfn,em,font,img,ins,kbd,q,s,samp,small,strike,strong,sub,sup,tt,var,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,table,caption,tbody,tfoot,thead,tr,th,td{margin:0;padding:0;border:0;outline:0;line-height:inherit;font-weight:inherit;font-style:inherit;font-size:100%;font-family:inherit;vertical-align:baseline}body{background:#000;font-family:monospace;font-size:18px;width:90%;height:90%;padding:5%}div{color:#fff;z-index:3;position:relative}canvas{width:auto!important;height:auto!important;position:fixed;top:0;left:0;z-index:2;background:transparent}::selection,::-moz-selection{color:#000;background:#fff}@media screen and (max-width:1200px){body{font-size:15px}}@media screen and (max-width:600px){body{font-size:14px}}@media screen and (max-width:350px){body{font-size:11px}}a{color:inherit;text-decoration:none}main a{text-decoration:underline}aside{position:fixed;top:5%;left:5%;top:5vh;left:5vh}h1,time{font-size:2.4em;font-weight:700;text-transform:uppercase;display:inline-block}h1:hover,li:hover,time:hover{transform:rotateY(180deg)}@media screen and (max-width:600px){h1,time{font-size:1em}}h2{font-size:12px}h4{font-weight:700;margin:6em 0 2em;display:inline-block}ul{list-style:none;text-transform:uppercase;font-weight:700}li{display:inline-block}nav:first-of-type ul{position:fixed;left:5%;bottom:5%;left:5vh;bottom:5vh;text-align:left}nav:last-of-type ul{position:fixed;right:5%;bottom:5%;right:5vh;bottom:5vh;text-align:right}main{width:100%;max-width:420px;margin:38.5vh auto}main ul,main nav ul:first-of-type,main nav ul:last-of-type{position:inherit;text-align:left;text-transform:none;font-weight:400}main p,main li{margin-bottom:1em;display:block}main li:hover{transform:none}main b,main strong{font-weight:700}main h2{font-weight:700;font-size:inherit}main p,main li,main h4,ul a{background:transparent}